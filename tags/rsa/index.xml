        &lt;span style=&#34;color:#66d9ef&#34;&gt;return&lt;/span&gt; signSignatures&lt;span style=&#34;color:#f92672&#34;&gt;[&lt;/span&gt;idx&lt;span style=&#34;color:#f92672&#34;&gt;];&lt;/span&gt;
